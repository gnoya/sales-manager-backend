generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model Product {
  id        String   @id @default(uuid())
  name      String
  quantity  Int
  price     Float
  profit    Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
